import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    static public String move (String str)
    {
        int len = str.length();
        String low = "";
        string upper = "";
        char ch;
        for (int i = 0; i < len; i++){
            ch = str.charAt(i);
            if (ch >= 'A' && ch <= 'Z'){
                upper += ch;

            }
            else{
                low += ch;
            }
        }
        return low + upper;
    }
    public static void main(String args[])
    {
        String str (InputFormat-1,InputFormat-2,InputFormat-3);
        System.out.println(after operation:    "+move(str"));
        

        /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    }
}
